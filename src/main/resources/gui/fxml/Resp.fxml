                        </ImageView>
                        <ImageView fitHeight="24.0" fitWidth="32.0" onMouseClicked="#deleteClicked" pickOnBounds="true" preserveRatio="true" style="-fx-cursor: hand;">
                          <image>
                            <Image url="@/gui/icons/icons/delete_icon24.png" />
                          </image>
                        </ImageView>
                      </children>
                      <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                      </padding>
                    </HBox>
                    <Text fx:id="message" fill="#cccccc" onMouseClicked="#respondClicked" scaleX="1.0" scaleY="1.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-cursor: hand;" text="Msg text goes here" textAlignment="JUSTIFY" wrappingWidth="370.0">
                      <font>
                        <Font name="SansSerif Bold" size="12.0" />
                      </font>
                    </Text>
                    <HBox alignment="BOTTOM_RIGHT" prefHeight="60.0" prefWidth="200.0" spacing="10.0">
