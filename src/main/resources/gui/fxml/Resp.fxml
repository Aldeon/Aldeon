<Pane fx:id="respPane" maxHeight="1.7976931348623157E308" pickOnBounds="true" prefHeight="169.99990000000253" prefWidth="640.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="org.aldeon.gui.controllers.ResponseController">
  <children>
    <HBox id="HBox" fx:id="windowContainer" alignment="CENTER" layoutX="5.0" layoutY="12.0" padding="$x1" spacing="5.0">
      <children>
        <StackPane id="StackPane">
          <children>
            <Rectangle fx:id="colorRectangle" arcHeight="20.0" arcWidth="20.0" fill="#5cff21" height="131.0001220703125" smooth="true" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="630.0">
              <effect>
                <DropShadow color="#0000009b" offsetY="5.0" />
              </effect>
            </Rectangle>
            <Rectangle fx:id="backgroundRectangle" arcHeight="0.0" arcWidth="0.0" fill="#222222" height="131.0001220703125" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="600.0" />
            <HBox id="HBox" alignment="TOP_RIGHT" spacing="5.0" StackPane.margin="$x1">
              <children>
                <VBox id="VBox" alignment="TOP_LEFT" prefWidth="400.0" spacing="5.0">
                  <children>
                    <HBox alignment="TOP_RIGHT" fillHeight="true" prefHeight="-1.0" prefWidth="350.0" spacing="6.0">
                      <children>
                        <ImageView fitHeight="24.0" fitWidth="24.0" onMouseClicked="#writeResponseClicked" pickOnBounds="true" preserveRatio="true" style="-fx-cursor: hand;">
