<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<VBox fx:id="mainWindow" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="org.aldeon.gui.controllers.TopicListController">
  <children>
    <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-1.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
      <children>
        <TextField fx:id="topicName" prefWidth="400.0" style="" />
        <Button mnemonicParsing="false" onMouseClicked="#createTopic" text="Create thread" />
      </children>
    </HBox>
    <HBox id="HBox" alignment="CENTER" spacing="5.0">
      <children>
        <FlowPane fx:id="fpane" alignment="CENTER" columnHalignment="CENTER" maxWidth="600.0" prefHeight="300.0" prefWidth="600.0" prefWrapLength="300.0">
          <padding>
            <Insets />
          </padding>
        </FlowPane>
      </children>
    </HBox>
  </children>
  <stylesheets>
    <URL value="@../css/style.css" />
  </stylesheets>
</VBox>
